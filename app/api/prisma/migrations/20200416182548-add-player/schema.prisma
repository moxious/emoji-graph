datasource DS {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = env("BINARY_TARGET")
}

model Emoji {
  id        String @id @default(cuid())
  code      String
  browser   String
  name      String
  category  String
}

model Story {
  id        String @id @default(cuid())
  emojis    String
  Story     String
  player    String
}

model Player {
  id        String @id @default(cuid())
  name      String
}
